{{ attach_library('milken_base/media_video') }}

{% set media_video_value =  media.toArray
  | merge({ 'entityTypeId': media.getEntityTypeId })
  | merge({ 'id': media.uuid.value })
  | merge({ 'drupal_internal__id': media.id })
  | merge({ 'includes': "thumbnail" })

%}


<media-video
  {{ attributes }}
  {{ content_attributes }}
  data-view-mode="card"
  data-entity="{{ media_video_value | jsonapi }}"
>

</media-video>
